io.value.play();\n    //   console.log(audio.value);\n    // });\n    // 时间\n    const hour = ref(0);\n    const minute = ref(0);\n    const second = ref(0);\n    onMounted(() => {\n      setInterval(() => {\n        let date = new Date();\n        hour.value = date.getHours();\n        minute.value = date.getMinutes();\n        second.value = date.getSeconds();\n      }, 1000);\n    });\n    return {\n      hour,\n      minute,\n      second,\n    };\n  },\n  components: {\n    SlideShow,\n    Profile,\n    Honor,\n    MyVideo,\n    PictureWall,\n    MyTimer,\n  },\n};\n</script>\n\n<style>\n/* 去掉了scoped 其样式可以被其子组件使用 */\nh2 {\n  font-weight: bold;\n  color: #333;\n  font-size: 30px;\n  margin: 0;\n}\na {\n  text-decoration: none;\n}\n#div-main-wrap {\n  width: 100%;\n}\n.h-title {\n  position: sticky;\n  display: flex;\n  top: 0px;\n  height: 80px;\n  z-index: 9999;\n  background-color: #154863;\n  /* border-bottom: 1px #777 solid; */\n  box-shadow: 0 1px 7px rgb(0 0 0 / 10%);\n  color: white;\n}\n.header {\n  display: flex;\n  position: absolute;\n  align-items: center;\n  margin: 10px;\n  color: #e5e38d;\n  font-size: 24px;\n  font-weight: bolder;\n  font-family: \"Montserrat\";\n}\n.header-pic {\n  width: 60px;\n  height: 60px;\n  background-image: url(\"../assets/img/logo.png\");\n  background-size: contain;\n  background-repeat: no-repeat;\n}\n.header-links {\n  position: absolute;\n  top: 30px;\n  right: 20px;\n}\n.signin {\n  font-size: 16px;\n  color: white;\n  margin-right: 20px;\n}\n.signup {\n  font-size: 16px;\n  color: #e5e38d;\n  padding: 4px 22px;\n  border: 2px solid;\n  border-radius: 100px;\n  border-color: #e5e38d;\n  font-weight: bold;\n}\n.navv {\n  margin: 10px auto 10px;\n  display: flex;\n  justify-content: center;\n  width: 900px;\n  padding: 10px 0px;\n  /* border-top: 1px #ddd solid; */\n}\n.navv a {\n  font-size: 18px;\n  padding: 0px 20px;\n  font-weight: bold;\n  color: white;\n}\n.main-area {\n  width: 100vw;\n  height: auto;\n  padding: 60px 0 0;\n  background-color: #f1f1f1;\n}\n.main-subheader {\n  width: 100%;\n  margin: 100px 0 100px 0;\n}\n.upload-button-area {\n  display: flex;\n  height: 100px;\n  font-size: 30px;\n  justify-content: center;\n}\n.button {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  font-size: 20px;\n  font-weight: bold;\n  width: 500px;\n  height: 70px;\n  border-radius: 100px;\n  background-color: #154863;\n  color: white;\n  border: 2px solid;\n}\n</style>\n"]}]}