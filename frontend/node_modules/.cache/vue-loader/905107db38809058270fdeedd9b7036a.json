       <h2 class=\"main-subheader\">\n          Generate personalized multiple-choice questions (MCQs) in seconds.\n        </h2>\n        <div class=\"upload-button-area\" href=\"#\">\n          <file-upload></file-upload>\n        </div>\n      </div>\n      <manual></manual>\n      <introduction></introduction>\n      <explore></explore>\n      <about-us></about-us>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref, onMounted } from \"vue\";\nimport FileUpload from \"../components/content/FileUpload.vue\";\nimport Manual from \"../components/content/Manual.vue\";\nimport Introduction from \"../components/content/Introduction.vue\";\nimport AboutUs from \"../components/content/AboutUs.vue\";\nimport Explore from \"../components/content/Explore.vue\";\nimport HeaderArea from \"../components/content/HeaderArea.vue\";\n\nexport default {\n  setup() {\n    // onMounted(() => {\n    //   // 监听全局点击事件\n    //   window.addEventListener(\"click\", clickA);\n    //   // audio.value.play();\n    //   console.log(audio.value);\n    // });\n    // 时间\n    const hour = ref(0);\n    const minute = ref(0);\n    const second = ref(0);\n    onMounted(() => {\n      setInterval(() => {\n        let date = new Date();\n        hour.value = date.getHours();\n        minute.value = date.getMinutes();\n        second.value = date.getSeconds();\n      }, 1000);\n    });\n    return {\n      hour,\n      minute,\n      second,\n    };\n  },\n  components: {\n    FileUpload,\n    Manual,\n    Introduction,\n    AboutUs,\n    Explore,\n    HeaderArea,\n  },\n};\n</script>\n\n<style scoped>\n/* 去掉了scoped 其样式可以被其子组件使用 */\n\n.main-area {\n  width: 100vw;\n  height: auto;\n  padding: 60px 0 0;\n  background-color: rgba(247, 114, 47, 0.08);\n}\n.main-subheader {\n  width: 100%;\n  margin: 50px 0 50px 0;\n}\n.upload-button-area {\n  display: flex;\n  height: 100px;\n  font-size: 30px;\n  justify-content: center;\n}\n.button {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  font-size: 20px;\n  font-weight: bold;\n  width: 500px;\n  height: 70px;\n  border-radius: 100px;\n  background-color: #154863;\n  color: white;\n  border: 2px solid;\n  cursor: pointer;\n}\n</style>\n"]}]}